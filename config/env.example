FAME_ENV=production
FAME_HEALTH_URL=http://127.0.0.1:8080/readyz

# LocalAI Configuration
USE_LOCALAI=true
LOCALAI_ENDPOINT=http://localhost:8080

# OpenAI / LocalAI
# Replace with your actual OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Google AI (Gemini) - For autonomous responses
GOOGLE_AI_KEY=your_google_ai_key_here

# Financial Data APIs
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
COINGECKO_API_KEY=your_coingecko_key_here
FINNHUB_API_KEY=your_finnhub_key_here

# Search & News APIs
SERPAPI_KEY=your_serpapi_key_here
SERPAPI_BACKUP_KEY=your_serpapi_backup_key_here
NEWSAPI_KEY=your_newsapi_key_here
GNEWS_API_KEY=your_gnews_key_here

# GitHub API
GITHUB_API_KEY=your_github_api_key_here

# AWS (if needed)
# Replace with your actual AWS credentials
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Voice Settings
ENABLE_VOICE=true

# ElevenLabs TTS (Premium Voice for FAME)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_elevenlabs_voice_id_here

# Optional APIs
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_CX=

